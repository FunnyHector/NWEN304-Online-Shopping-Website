<html>
<% include components/head.ejs %>
<body>
<% include components/menu_and_heading.ejs %>
<div class="container content">
    <h1 class="login-heading"><%= title%></h1>
    <div class="register-panel left">
        <% if (message.length > 0) { %>
        <div class="alert alert-success"><%= message %></div>
        <% } else if (locals.error) { %>
        <div class="alert alert-danger" role="alert"><%= locals.error %></div>
        <% } %>
        <label>First Name: </label> <p><%= first_name %></p>
        <label>Last Name: </label> <p><%= last_name %></p>
        <label>Email: </label><p><%= email %></p>
        <label>Contact No: </label><p><%= contact_no %></p>
        <label>Street: </label><p><%= street %></p>
        <label>Suburb: </label><p><%= suburb %></p>
        <label>City: </label><p><%= city %></p>
        <label>Country: </label><p><%= country %></p>

        <a href="/profile_update/<%= username%>" class="btn btn-primary btn-large max-width" role="button">Edit</a>
    </div>
</div>
</body>
</html>